#%RAML 1.0 DataType
type: object
properties: 
  catalog:
    type: array
    items:
      properties:
        artist: 
          type: array
          required: false
          items: 
            type: artist
        song:
          type: array
          required: false
          items: 
            properties:
              type: song
              songWriters:
                type: array
                required: false
                items:
                  type: songWriter